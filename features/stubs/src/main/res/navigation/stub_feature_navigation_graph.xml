        tools:layout="@layout/fragment_bottom_navigation_stub">
        <argument
            android:name="destination"
            app:argType="integer"
            android:defaultValue="3"/>
    </fragment>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="io.captaingaga.airtickets.effective.mobile.stub.BottomNavigationStubFragment"
