<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/calendar_navigation"
    app:startDestination="@id/calendarFragment">

    <fragment
        android:id="@+id/calendarFragment"
        android:name="io.captaingaga.calendar.CalendarFragment"
        android:label="fragment_calendar"
        tools:layout="@layout/fragment_calendar" >
        <action
            android:id="@+id/action_calendarFragment_to_innerCalendarFragment"
            app:destination="@id/innerCalendarFragment" />
    </fragment>
    <fragment
        android:id="@+id/innerCalendarFragment"
        android:name="io.captaingaga.calendar.InnerCalendarFragment"
        android:label="fragment_inner_calendar"
        tools:layout="@layout/fragment_inner_calendar" />
</navigation>