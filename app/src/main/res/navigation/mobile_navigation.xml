<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_main">

    <include app:graph="@navigation/main_feature_navigation_graph" />
    <fragment
        android:id="@+id/navigation_hotels"
        android:name="io.captaingaga.airtickets.effective.mobile.ui.HotelsFragment"
        android:label="fragment_hotels"
        tools:layout="@layout/fragment_hotels" />
    <fragment
        android:id="@+id/navigation_briefly"
        android:name="io.captaingaga.airtickets.effective.mobile.ui.BrieflyFragment"
        android:label="fragment_briefly"
        tools:layout="@layout/fragment_briefly" />
    <fragment
        android:id="@+id/navigation_subscriptions"
        android:name="io.captaingaga.airtickets.effective.mobile.ui.SubscriptionsFragment"
        android:label="fragment_subscriptions"
        tools:layout="@layout/fragment_subscriptions" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="io.captaingaga.airtickets.effective.mobile.ui.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />


</navigation>