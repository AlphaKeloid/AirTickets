<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- Semibold -->
    <font
        app:font="@font/sf_pro_display_semibold"
        app:fontStyle="normal"
        app:fontWeight="600" />
    <font
        app:font="@font/sf_pro_display_semibolditalic"
        app:fontStyle="italic"
        app:fontWeight="600" />

    <!-- Medium -->
    <font
        app:font="@font/sf_pro_display_medium"
        app:fontStyle="normal"
        app:fontWeight="500" />
    <font
        app:font="@font/sf_pro_display_mediumitalic"
        app:fontStyle="italic"
        app:fontWeight="500" />

    <!-- Regular -->
    <font
        app:font="@font/sf_pro_display_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />
    <font
        app:font="@font/sf_pro_display_regularitalic"
        app:fontStyle="italic"
        app:fontWeight="400" />
</font-family>
